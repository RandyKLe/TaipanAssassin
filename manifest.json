{

  "manifest_version": 1,
  "name": "TaipanAssassin",
  "version": "1.0",

  "description": "Quickly aquires item matching given description.",
  "homepage_url": "https://github.com/WickedWallaby/TaipanAssassin",
  "icons": {
    "48": "icons/beasts-48.png"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/beasts-32.png",
    "default_title": "TaipanAssassin",
    "default_popup": "popup/choose_beast.html"
  },

  "web_accessible_resources": [
    "beasts/frog.jpg",
    "beasts/turtle.jpg",
    "beasts/snake.jpg"
  ]

}